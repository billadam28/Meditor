<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="MeditorPersistence.GeographicalArea" table="Geographical_Area" catalog="meditorDb" optimistic-lock="version">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="geoName" type="string">
            <column name="geo_name" length="50" />
        </property>
    </class>
</hibernate-mapping>
